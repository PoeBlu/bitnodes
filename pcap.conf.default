[pcap]

# Logfile
logfile = pcap.log

# Print debug output
debug = True

# Redis TTL for cached messages
ttl = 10800

# Number of round-trip time (RTT) values to keep for each node
rtt_count = 15

# Tor proxy
tor_proxy = 127.0.0.1:9050

# Relative path to directory containing pcap files
pcap_dir = data/pcap
